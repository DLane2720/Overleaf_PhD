  author = {{Castano-Londono}, Luis and Marrugo Llorente, Stefany del Pilar and {Paipa-Sanabria}, Edwin and {Orozco-Lopez}, Mar{\'i}a Bel{\'e}n and Fuentes Monta{\~n}a, David Ignacio and Gonzalez Montoya, Daniel},
  year = 2024,
