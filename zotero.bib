  shorttitle = {Sensor and {{Sensor Fusion Technology}} in {{Autonomous Vehicles}}},
  author = {Yeong, De Jong and {Velasco-Hernandez}, Gustavo and Barry, John and Walsh, Joseph},
  year = 2021,
